import java.awt.*;
import java.applet.*;
import java.util.*;

public class GrassHills extends Applet
{
  public static void drawPathHill(Graphics g)
  {
      g.setColor(new Color(76,153,0));
      g.fillArc(375,550,1200,500,90,90);
   }
  public static void drawHouseHill(Graphics g)
  {
      g.setColor(new Color(76,153,0));
      g.fillArc(745,440,1450,825,0,180);
   }
}
