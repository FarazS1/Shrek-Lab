//Lab 007 dab dab 
//Shrek's swamp
import java.awt.*;
import java.applet.*;
import java.awt.Graphics;
import java.util.*;

public class Main extends Applet 
{
    public void paint(Graphics g)
   {
    Background.drawSky(g);
    Background.drawTreeA(g);
    Background.drawTreeB(g);
    Background.drawLeavesA(g);
    Background.drawLeavesB(g);
    GrassHills.drawPathHill(g, 390, 550, 90, 90);
    GrassHills.drawHouseHill(g, 745, 440, 0, 180);
    Sand.setGround(g);
    OgreSign.drawStand(g);
    OgreSign.drawSign(g);
    OgreSign.drawWordBeware(g);
    OgreSign.drawOgre(g);
    TreeHouse.drawBuilding(g);
    TreeHouse.drawTreeTrunk(g);
    TreeHouse.drawDoor(g);
    TreeHouse.drawInside(g);
    TreeHouse.drawWindow(g);
    
    }
}
